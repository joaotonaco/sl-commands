{
	"name": "sl-commands",
	"version": "2.0.9",
	"description": "A simple handler for discord.js v14.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"latest": "pnpm build && pnpm publish && git push",
		"beta": "pnpm version patch && pnpm build && pnpm publish --tag beta && git push",
		"test": "prettier -w test/**/*.ts && cls && ts-node-dev test/index.test.ts",
		"build": "tsc && cls"
	},
	"keywords": [
		"discord",
		"bot",
		"context",
		"command",
		"event",
		"handler",
		"typescript",
		"class"
	],
	"author": "bluey#0012",
	"license": "MIT",
	"dependencies": {
		"@sapphire/shapeshift": "^3.5.1",
		"chalk": "4.1.2",
		"discord.js": "^14.2.0",
		"dotenv": "^16.0.1",
		"mongoose": "^6.5.2",
		"ts-mixer": "^6.0.1",
		"tslib": "^2.4.0",
		"typed-emitter": "^2.1.0"
	},
	"devDependencies": {
		"@types/node": "^18.7.6",
		"prettier": "^2.7.1",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"typescript": "^4.7.4"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bluee-js/sl-commands.git"
	},
